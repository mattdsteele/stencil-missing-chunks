{
    "name": "sample-app",
    "version": "0.1.0",
    "description": "Stencil Component Starter",
    "main": "dist/up_elements.js",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "files": [
        "dist/"
    ],
    "browser": "dist/up_elements.js",
    "scripts": {
        "build": "stencil build --dev --es5 --stats",
        "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
        "start": "npm run dev --es5 --stats",
        "test": "jest --no-cache",
        "test.watch": "jest --watch --no-cache"
    },
    "dependencies": {
        "@stencil/core": "0.5.0-2"
    },
    "devDependencies": {
        "@stencil/dev-server": "latest",
        "@stencil/utils": "latest",
        "@types/jest": "^21.1.1",
        "globby": "^7.1.1",
        "jest": "^21.2.1",
        "jest-junit": "^3.3.0",
        "rimraf": "^2.6.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ionic-team/stencil-component-starter.git"
    },
    "author": "Ionic Team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ionic-team/stencil"
    },
    "homepage": "https://github.com/ionic-team/stencil",
    "jest": {
        "coverageReporters": [
            "cobertura"
        ],
        "collectCoverage": true,
        "coverageDirectory": "target/coverage",
        "testResultsProcessor": "jest-junit",
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/../../node_modules/@stencil/core/testing/jest.preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json",
            "jsx"
        ]
    },
    "jest-junit": {
        "output": "target/junit/junit.xml"
    }
}
